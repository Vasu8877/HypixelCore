{
    "name": "biswajit/skyblockcore",
    "description": "Skyblock plugin for PocketMine",
    "type": "pocketmine-plugin",
    "require": {
        "php": ">=8.2",
        "pocketmine/pocketmine-mp": "^5.0",
        "dktapps/pmforms": "^2.1",
        "sof3/libasynql": "^4.0",
        "muqsit/invmenu": "^4.7",
		"ext-zip": "*",
		"ext-gd": "*"
	},
    "autoload": {
        "psr-4": {
            "Biswajit\\": "src/Biswajit/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src",
        "check": [
            "@analyse",
            "@test"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
